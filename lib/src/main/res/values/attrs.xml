<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--手势解锁view的自定义属性-->
    <declare-styleable name="LjyGestureLockView">
        <attr name="iconPointUncheck" format="reference"/>
        <attr name="iconPointCheck" format="reference"/>
        <attr name="iconPointError" format="reference"/>
        <attr name="iconLinePressed" format="reference"/>
        <attr name="iconLineError" format="reference"/>
    </declare-styleable>
    <!--数字密码view的自定义属性-->
    <declare-styleable name="LjyPwdInputView">
        <attr name="pivBorderColor" format="color"/>
        <attr name="pivBorderWidth" format="dimension"/>
        <attr name="pivBorderRadius" format="dimension"/>
        <attr name="pivPasswordColor" format="color"/>
        <attr name="pivPasswordWidth" format="dimension"/>
        <attr name="pivPasswordRadius" format="dimension"/>
        <attr name="pivPasswordLength" format="integer"/>
    </declare-styleable>
    <!--雷达图的属性-->
    <declare-styleable name="LjyRadarView">
        <attr name="count" format="integer"/>
        <attr name="layerCount" format="integer" />
        <attr name="mDrawableSize" format="dimension|reference" />
        <attr name="mDrawablePadding" format="dimension|reference" />
        <attr name="descPadding" format="dimension|reference" />
        <attr name="titleSize" format="dimension|reference"/>
        <attr name="dataSize" format="dimension|reference" />
        <attr name="radarPercent" format="float"/>
        <attr name="startColor" format="color|reference" />
        <attr name="endColor" format="color|reference" />
        <attr name="cobwebColor" format="color|reference" />
        <attr name="lineColor" format="color|reference" />
        <attr name="dataColor" format="color|reference" />
        <attr name="singleColor" format="color|reference" />
        <attr name="titleColor" format="color|reference" />
        <attr name="radarPointColor" format="color|reference" />
        <attr name="raderBorderColor" format="color|reference" />
        <attr name="radiusColor" format="color|reference" />
        <attr name="raderBorderWidth" format="dimension|reference" />
        <attr name="pointRadius" format="dimension|reference" />
        <attr name="enabledBorder" format="boolean"/>
        <attr name="enabledAnimation" format="boolean"/>
        <attr name="enabledShowPoint" format="boolean"/>
        <attr name="enabledPolygon" format="boolean"/>
        <attr name="enabledShade" format="boolean"/>
        <attr name="enabledRadius" format="boolean"/>
        <attr name="enabledText" format="boolean"/>
        <attr name="animDuration" format="integer"/>
    </declare-styleable>
    <!--gifImageView的自定义属性-->
    <declare-styleable name="GIFVIEW">
        <!--gif文件引用-->
        <attr name="gifSrc" format="reference"  />
        <!--是否加载完自动播放-->
        <attr name="authPlay" format="boolean"  />
        <!--播放次放，默认永远播放-->
        <attr name="playCount" format="integer"  />
    </declare-styleable>
</resources>